  "dependencies": {
    "axios": "0.19.2",
    "dayjs": "1.8.26",
    "element-ui": "2.13.1",
    "nuxt": "2.12.2",
    "vue-scrollto": "2.18.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "2.0.2",
    "@nuxtjs/style-resources": "1.0.0",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.5.1",
    "browser-env": "3.3.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.5.4",
    "node-sass": "4.14.0",
    "require-extension-hooks-babel": "1.0.0",
    "sass-loader": "8.0.2",
    "stylelint": "13.3.3",
