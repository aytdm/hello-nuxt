    "analyze": "nuxt build --analyze",
    "test": "jest --verbose",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "axios": "0.19.2",
    "dayjs": "1.8.30",
    "element-ui": "2.13.2",
    "nuxt": "2.13.3",
    "vue-scrollto": "2.18.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "3.0.0",
    "@nuxtjs/style-resources": "1.0.0",
    "@vue/test-utils": "1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "browser-env": "3.3.0",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.22.0",
    "jest": "26.1.0",
    "node-sass": "4.14.1",
    "require-extension-hooks-babel": "1.0.0",
    "sass-loader": "8.0.2",
    "stylelint": "13.6.1",
    "stylelint-scss": "3.18.0",
    "vue-jest": "3.0.6"
  }
