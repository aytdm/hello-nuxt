{
  "name": "hello-nuxt",
  "version": "1.3.2",
  "description": "Nuxt.js project",
  "author": "numa",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint --fix \"src/**/*.{scss,vue}\"",
